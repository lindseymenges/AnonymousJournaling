<div class="new_entry_elements">
<h3>Write Your New Entry</h3>
<%= form_for @entry do |f| %>
  <%= f.label :title %><br>
  <%= f.text_field :title %><br>
  <%= f.label :content %><br>
  <%= f.text_area :content, :wrap => :hard %><br>
  <%= f.submit %>
<% end %>
<%= link_to 'Return Home', root_path %>
</div>
